{
  "name": "partall-list",
  "private": true,
  "license": "GPL-3.0",
  "author": "Johannes Andersen",
  "description": "This is a filter list specifically made for <a href=\"https://github.com/gorhill/uBlock\">uBlock Origin (uBO)</a> The list is mainly aimed at Norwegian users and websites.",
  "scripts": {
    "lint": "aglint",
    "lint:fix": "aglint --fix",
    "codeowners": "codeowners-generator generate",
    "codeowners:check": "codeowners-generator generate --check"
  },
  "engines": {
    "node": "21",
    "npm": "10"
  },
  "dependencies": {
    "@adguard/aglint": "2.0.6",
    "codeowners-generator": "2.3.1"
  },
  "codeowners-generator": {
    "includes": [
      "**/CODEOWNERS"
    ],
    "output": ".github/CODEOWNERS",
    "groupSourceComments": true,
    "customRegenerationCommand": "npm run codeowners"
  }
}
